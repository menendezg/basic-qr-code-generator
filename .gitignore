# Ignore bundler config
/.bundle

# Ignore the directory where gems are installed by Bundler
/vendor/bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
/pid
*.pid

# Ignore cache and temp files.
/cache/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore dotenv files (if you're using dotenv for environment variables)
.env
.env.*

# Ignore database files
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore compiled files
/*.o
/*.a
/*.so

# Ignore database schema dumps
/db/schema.rb
/db/structure.sql

# Ignore byebug command history file.
.byebug_history

# Ignore secrets
/config/credentials.yml.enc
/config/master.key

# Ignore RuboCop artifacts
/.rubocop-http--*
/.rubocop_cache/*

# Ignore the coverage directory generated by simplecov
/coverage/

# Ignore macOS folder attributes
.DS_Store

# Ignore Vim swap files
*.swp

# Ignore RubyMine IDE project

